{
  "builds": [
    {
      "src": "project4/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "STATICFILES_DIRS" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project4/wsgi.py"
    },
    {
      "src": "network/static/(.*)",
      "dest": "network/static/$1"
    }
  ]
}
